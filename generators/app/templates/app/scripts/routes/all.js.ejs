/*global <%= appSlugName %>, Backbone*/
<%= appSlugName %>.Routers = <%= appSlugName %>.Routers || {};

(function () {
    'use strict';
    <%= appSlugName %>.Routers.All = Backbone.Router.extend({
        routes: {
            '':'main',
            'about':'about'
        },
        initialize:function(){
        },
        execute: function(callback, args, name) {
            if (callback) callback.apply(this, args);
        },
        main: function () {
            new <%= appSlugName %>.Views.Portal();
        }
    });

    <%= appSlugName %>.Routers.Router = new <%= appSlugName %>.Routers.All();
    Backbone.history.start();
})();
